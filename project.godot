config_version=5

[addons]

resources_spreadsheet_view/array_color_tint=100.0
resources_spreadsheet_view/color_rows=true
resources_spreadsheet_view/array_min_width=128.0
resources_spreadsheet_view/resource_preview_size=32.0
resources_spreadsheet_view/clip_headers=false
resources_spreadsheet_view/dupe_arrays=true
resources_spreadsheet_view/context_menu_on_leftclick=false
resources_spreadsheet_view/fold_docks=true
resources_spreadsheet_view/resource_cell_label_mode=1
resources_spreadsheet_view/freeze_first_column=true

[application]

config/name="Don't Kill The Crew"
config/description="Originally developed for the Low Rez Game Jam 2024"
config/version=".2"
config/tags=PackedStringArray("game_jam", "low_rez_jam")
run/main_scene="res://root/scenes/MasterScene.tscn"
config/features=PackedStringArray("4.4", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/use_filter=false
boot_splash/image="res://Title_DKTC.png"
config/icon="res://Title_DKTC.png"

[audio]

buses/default_bus_layout="res://root/resources/default_bus_layout.tres"

[autoload]

Settings="*res://root/autoload/Settings.gd"
Scenes="*res://addons/scene_manager/scenes.gd"
Crew="*res://root/scripts/crew.gd"
Log="*res://addons/logger/logger.gd"
SceneManager="*res://addons/scene_manager/scene_manager.tscn"
DebugMenu="*res://addons/debug_menu/debug_menu.tscn"

[display]

window/size/viewport_width=768
window/size/viewport_height=768
window/size/initial_position_type=3
window/stretch/mode="canvas_items"
window/stretch/scale_mode="integer"
mouse_cursor/custom_image="res://root/assets/sprites/gui/Cursor.png"
mouse_cursor/tooltip_position_offset=Vector2(2, 2)

[dotnet]

project/assembly_name="Low Rez Jam"

[editor_plugins]

enabled=PackedStringArray("res://addons/crt/plugin.cfg", "res://addons/debug_menu/plugin.cfg", "res://addons/format_on_save/plugin.cfg", "res://addons/gdLinter/plugin.cfg", "res://addons/logger/plugin.cfg", "res://addons/resources_spreadsheet_view/plugin.cfg", "res://addons/scene_manager/plugin.cfg", "res://addons/script-ide/plugin.cfg")

